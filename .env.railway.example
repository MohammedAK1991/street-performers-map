# Railway Production Environment Variables Template
# Copy these to Railway Dashboard and fill in your actual values

NODE_ENV=production
PORT=3001
API_VERSION=v1

# Database - Add your actual MongoDB connection string
MONGODB_URI=mongodb+srv://USERNAME:PASSWORD@cluster.mongodb.net/DATABASE_NAME

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRES_IN=7d

# Google Maps API - Add your actual key
GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# File Upload Settings
MAX_FILE_SIZE=100MB

# Cloudinary Configuration - Add your actual values
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info

# Stripe Configuration - Add your actual keys
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret

# Clerk Authentication - Add your actual secret key
CLERK_SECRET_KEY=sk_test_your_clerk_secret_key